! Black color that will not be affected by bold highlighting.
*.color66: #0e1114
*color66:  #0e1114

! Xclock colors.
XClock*foreground: #9ecddb
XClock*background: #0e1114
XClock*majorColor:  rgba:9e/cd/db/ff
XClock*minorColor:  rgba:9e/cd/db/ff
XClock*hourColor:   rgba:9e/cd/db/ff
XClock*minuteColor: rgba:9e/cd/db/ff
XClock*secondColor: rgba:9e/cd/db/ff

!! Set depth to make transparency work.
URxvt*depth: 32

URxvt.intensityStyles:	false
URxvt.font: 		xft:monospace:size=11

!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1
