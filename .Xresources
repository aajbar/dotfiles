! Black color that will not be affected by bold highlighting.
*.color66: #0E081F
*color66:  #0E081F

! Xclock colors.
XClock*foreground: #e9c8ea
XClock*background: #0E081F
XClock*majorColor:  rgba:e9/c8/ea/ff
XClock*minorColor:  rgba:e9/c8/ea/ff
XClock*hourColor:   rgba:e9/c8/ea/ff
XClock*minuteColor: rgba:e9/c8/ea/ff
XClock*secondColor: rgba:e9/c8/ea/ff

! Set depth to make transparency work.
URxvt*depth: 32

URxvt.intensityStyles:	false
URxvt.font: 		xft:monospace:size=11
URxvt.scrollBar:	false

!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1
